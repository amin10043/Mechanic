                    android:layout_marginTop="10dp"
                    android:background="#64bc2c"
                    android:text="ویرایش"
                    android:textColor="@color/white" />

                <Button
                    android:id="@+id/shareIntroduction"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/ImgbtnEdit"
                    android:layout_marginTop="10dp"
                    android:background="@color/blue2"
                    android:text="اشتراک گذاری"
                    android:textColor="@color/white"
                    android:textSize="15dp" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="20dp"
                    android:orientation="horizontal" >

                    <RelativeLayout
                        android:id="@+id/countLiketext"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="10dp" >

                        <TextView
                            android:id="@+id/labelPoepleFollowed"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="دنبال کنندگان"
                            android:textColor="@color/transparent" />

                        <TextView
                            android:id="@+id/countLikeIntroduction"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/labelPoepleFollowed"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="10dp"
                            android:text="12"
                            android:textColor="@color/transparent" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="10dp" >

                        <TextView
                            android:id="@+id/labelviewObject"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="بازدید"
                            android:textColor="@color/transparent" />

                        <TextView

                            android:layout_height="wrap_content"
                            android:layout_below="@+id/labelviewObject"
                            android:layout_centerHorizontal="true"
                            android:layout_marginTop="10dp"
                            android:text="70"
                            android:textColor="@color/transparent" />
                    </RelativeLayout>

